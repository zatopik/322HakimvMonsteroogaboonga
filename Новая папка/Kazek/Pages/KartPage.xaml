<Page x:Class="Kazek.Pages.KartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kazek.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="KartPage">

    <Grid>
        <Image Margin="0,0,10,10" Source="/Pages/grunge-black-concrete-textured-background.jpg" Stretch="Fill"/>
        <Grid/>
        <Button Content="Назад" Margin="24,10,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <ListView x:Name="listv" HorizontalAlignment="Left" Height="202" Margin="114,119,0,0" VerticalAlignment="Top" Width="528" d:ItemsSource="{d:SampleData ItemCount=5}">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Page>
